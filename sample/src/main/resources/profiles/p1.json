{
    test: {
        kind: "parallel",
        time: [5, "seconds"],
        count: 10,
        init: 1,
        incr: 1
    },

    service: "sample-svc",
    client: "sample-svc",

    graph: {
        title: "Service Performance",
        xlabel: "Parallel Requests",

        // following 2 arrays should have the same size: M
        ylabels: ["MicroSeconds", "Per Second"],
        ysizes: [60, 30],

        // following 3 arrays should have the same size: N
        labels: ["Client", "Server", "Requests"],
        where: [0, 0, 1],    // values in range 0..M-1
        fields: [
            ["client", "hist", "latency", "mean"],
            ["server", "hist", "server.latency.microsecs", "mean"],
            ["client", "gauges", "count"]
        ]
    },

    // following will contain test results
    vals: null,

    requests: {
        "*": "sample-data"
    }
}
